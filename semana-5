CREATE TABLE `detalles_de_ventas` (
  `id_destalle_de_ventas` int NOT NULL,
  `Ventas_id_venta` int NOT NULL,
  `Productos_id_producto` int NOT NULL,
  `cantidad_de_producto` varchar(50) DEFAULT NULL,
  `precio` varchar(100) DEFAULT NULL,
  `total_por_producto` varchar(100) DEFAULT NULL,
  PRIMARY KEY (`id_destalle_de_ventas`),
  KEY `fkDetalles_De_Ventas_Ventas` (`Ventas_id_venta`),
  KEY `fkDetalles_De_Ventas_Productos` (`Productos_id_producto`),
  CONSTRAINT `fkDetalles_De_Ventas_Productos` FOREIGN KEY (`Productos_id_producto`) REFERENCES `productos` (`id_producto`),
  CONSTRAINT `fkDetalles_De_Ventas_Ventas` FOREIGN KEY (`Ventas_id_venta`) REFERENCES `ventas` (`id_venta`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

