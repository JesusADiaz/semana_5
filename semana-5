CREATE TABLE `ventas` (
  `id_venta` int NOT NULL,
  `fecha_de_ventas` date DEFAULT NULL,
  `total_de_venta` varchar(100) DEFAULT NULL,
  `Clientes_id_cliente` int NOT NULL,
  PRIMARY KEY (`id_venta`),
  KEY `fkVentas_Clientes` (`Clientes_id_cliente`),
  CONSTRAINT `fkVentas_Clientes` FOREIGN KEY (`Clientes_id_cliente`) REFERENCES `clientes` (`id_cliente`)
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_0900_ai_ci

